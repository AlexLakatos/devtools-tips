---js
{
  title: "DevTools Tips",
  layout: "home.njk",
  templateEngineOverride: "njk",
  eleventyExcludeFromCollections: true,
  pagination: {
      data: "collections",
      size: 1,
      alias: "browser",
      before: function(data) {
        return data.filter(entry => entry.startsWith('browser:'));
      }
  },
  permalink: "browser/{{ browser | slug | processBrowserTagName }}/"
}
---
<main>
  <h2>Tips supported in {{ browser | processBrowserTagName }}</h2>
  <ul class="tips">
    {%- for post in collections[browser] -%}
    <li class="hover-box tip"><a href="{{ post.url }}">{{ post.data.title }}</a></li>
    {%- endfor -%}
  </ul>
</main>